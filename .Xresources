! special
*.foreground:   #d0d0d0
*.background:   #151515
*.cursorColor:  #d0d0d0

! black
*.color0:	#151515
*.color8:	#505050

! red
*.color1:	#ac4142
*.color9:	#ac4142

! green
*.color2:	#90a959
*.color10:	#90a959

! yellow
*.color3:	#f4bf75
*.color11:	#f4bf75

! blue
*.color4:	#6a9fb5
*.color12:	#6a9fb5

! magenta
*.color5:	#aa759f
*.color13:	#aa759f

! cyan
*.color6:	#75b5aa
*.color14:	#75b5aa

! white
*.color7:	#d0d0d0
*.color15:	#f5f5f5

URxvt*cursorBlink: true
!URxvt*font:  xft:Space Mono:size=12:antialias=true
URxvt*font:  xft:Bitstream Vera Sans Mono:size=12:antialias=true
!URxvt*font:  xft:Jetbrains Mono:size=12:antialias=true
urxvt*background: rgba:0000/0000/0100/c900
URxvt*depth: 32
URxvt*scrollBar: false
URxvt.internalBorder: 8
Xcursor.size: 24
URxvt.keysym.Shift-Control-V: eval:paste_clipboard
URxvt.keysym.Shift-Control-C: eval:selection_to_clipboard
URxvt.perl-ext-common: default,resize-font
URxvt.keysym.C-minus:     resize-font:smaller
URxvt.keysym.C-plus:	  resize-font:bigger
URxvt.keysym.C-equal:     resize-font:reset
URxvt.keysym.C-question:  resize-font:show
